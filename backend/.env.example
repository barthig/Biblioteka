### Copy to .env.local and adjust
APP_ENV=dev
APP_SECRET=change_me
APP_VERSION=1.0.0
DATABASE_URL="postgresql://biblioteka:biblioteka@127.0.0.1:5432/biblioteka_dev?serverVersion=16&charset=utf8"
API_SECRET=change_me_api
JWT_SECRET=change_me_jwt
JWT_SECRETS=change_me_jwt1,change_me_jwt2,change_me_jwt3
MESSENGER_TRANSPORT_DSN=amqp://app:app@localhost:5672/%2f/messages
MAILER_DSN=null://null
PORT=8000
OPENAI_API_KEY=change_me_openai_key

# Sentry Monitoring (opcjonalne - zostaw puste aby wyłączyć)
# Uzyskaj DSN z https://sentry.io/settings/projects/
SENTRY_DSN=

# Redis Cache Configuration
# Format: redis://host:port/database
# Domyślnie Redis używa portu 6379
REDIS_URL=redis://localhost:6379
REDIS_CACHE_DSN=redis://localhost:6379/0

# Elasticsearch Configuration
# Format: host:port
# Domyślnie Elasticsearch używa portu 9200
ELASTICSEARCH_HOST=localhost:9200

CORS_ALLOW_ORIGIN=*
