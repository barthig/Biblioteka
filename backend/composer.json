{
  "name": "barthig/biblioteka-backend",
  "type": "project",
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^6.4",
    "symfony/orm-pack": "^2.2",
    "doctrine/doctrine-bundle": "^2.8",
    "symfony/messenger": "^6.4",
    "symfony/amqp-messenger": "^5.4",
    "symfony/maker-bundle": "^1.52",
    "doctrine/doctrine-fixtures-bundle": "^4.4",
    "symfony/serializer-pack": "^1.3",
    "symfony/mailer": "^7.3",
    "symfony/notifier": "^7.3",
    "nelmio/api-doc-bundle": "^4.5",
    "symfony/validator": "^6.4",
    "symfony/rate-limiter": "^7.4",
    "symfony/lock": "^7.4",
    "sentry/sentry-symfony": "^5.8",
    "symfony/cache": "^7.4",
    "symfony/redis-messenger": "^5.4",
    "predis/predis": "^3.3",
    "overblog/graphql-bundle": "^1.7",
    "friendsofsymfony/elastica-bundle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "symfony/debug-bundle": "^6.4",
    "phpunit/phpunit": "^10.0",
    "symfony/browser-kit": "^7.3",
    "symfony/css-selector": "^7.3"
  },
  "scripts": {
    "start": "symfony server:start",
    "test": "php vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
